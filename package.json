    "express": "^4.17.1",
    "express-jwt": "^7.7.5",
    "node-cache": "^5.1.2",
    "tslib": "^2.4.0",
    "vue": "^3.2.19",
    "vue-class-component": "^8.0.0-0",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.3",
