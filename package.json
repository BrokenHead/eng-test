    "@vue/eslint-config-typescript": "^5.0.2",
    "concurrently": "^6.5.1",
    "eslint": "^7.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-0",
    "jest": "^29.0.3",
